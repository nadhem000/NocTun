{
	"name": "NOC Tunisian Chapter",
	"short_name": "NOCTun",
	"id": "NOCTun",
	"start_url": "/",
	"description": "موقع الفرع التونسي للاتحاد الفلكي العالمي. نزل التطبيق لتبقى  على اطلاع.",
	"display": "standalone",
	"background_color": "#ffffff",
	"theme_color": "#0056b3",
	"orientation": "portrait",
	"handle_links": "preferred",
	"edge_side_panel": {
		"preferred_width": 400
	},
	"display_override": [
		"window-controls-overlay",
		"standalone",
		"browser"
	],
	"launch_handler": {
		"client_mode": "auto"
	},
	"categories": ["science", "education", "kids"],
	"dir": "auto",
	"iarc_rating_id": "e58c174a-81d2-5c3c-32cc-34b8de4a52e9",
	"lang": "ar",
	"prefer_related_applications": false,
	"scope": "/",
	"icons": [
		{
			"src": "assets/icons/noc-logo-512x512.png",
			"sizes": "512x512",
			"type": "image/png",
			"purpose": "any"
		},
		{
			"src": "assets/icons/noc-logo-192x192.png",
			"sizes": "192x192",
			"type": "image/png",
			"purpose": "maskable"
		}
	],
	"screenshots": [
		{
			"src": "assets/screenshots/screenshot_01.png",
			"sizes": "512x512",
			"type": "image/png",
			"platform": "wide"
		},
		{
			"src": "assets/screenshots/screenshot_02.png",
			"sizes": "512x512",
			"type": "image/png",
			"platform": "wide"
		},
		{
			"src": "assets/screenshots/screenshot_03.png",
			"sizes": "512x512",
			"type": "image/png",
			"platform": "wide"
		}
	],
	"file_handlers": [
		{
			"action": "/open-pdf",
			"accept": {
				"application/pdf": [".pdf"]
			},
			"icons": [
				{
					"src": "assets/icons/pdf-icon-256x256.png",
					"sizes": "256x256",
					"type": "image/png"
				}
			],
			"launch_type": "single-client"
		},
		{
			"action": "/view-image",
			"accept": {
				"image/*": [".jpg", ".jpeg", ".png", ".gif", ".webp"]
			},
			"icons": [
				{
					"src": "assets/icons/image-icon-256x256.png",
					"sizes": "256x256",
					"type": "image/png"
				}
			],
			"launch_type": "multiple-clients"
		},
		{
			"action": "/play-video",
			"accept": {
				"video/*": [".mp4", ".webm", ".ogg"],
				"application/x-mpegURL": [".m3u8"]
			},
			"icons": [
				{
					"src": "assets/icons/video-icon-256x256.png",
					"sizes": "256x256",
					"type": "image/png"
				}
			],
			"launch_type": "single-client"
		}
	],
	"related_applications": [
		{
			"platform": "windows",
			"url": "https://noc-tunisian-chapter.netlify.app/contact.html",
			"id": "example.ExampleApp"
		},
		{
			"platform": "play",
			"url": "https://noc-tunisian-chapter.netlify.app/contact.html"
		}
	],
	"protocol_handlers": [
		{
			"protocol": "web+music",
			"url": "/play?track=%s"
		}
	]
}